{
    "name": "pandawa/skeleton",
    "description": "A minimal Pandawa project recommended to create applications.",
    "keywords": ["framework", "pandawa"],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "inertiajs/inertia-laravel": "^0.6.3",
        "laravel/horizon": "^5.10",
        "laravel/tinker": "^2.7",
        "pandawa/eloquent-bundle": "^5.0",
        "pandawa/foundation-bundle": "^5.0",
        "pandawa/routing-bundle": "^5.0",
        "pandawa/view-bundle": "^5.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "^1.9",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^9.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Bima\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        },
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "symfony/thanks": true
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "/Users/iqbalmaulana/Codes/pandawa/pandawa/src/Bundle/ViewBundle"
        }
    ]
}
